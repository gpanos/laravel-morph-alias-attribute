{
    "name": "gpanos/laravel-morph-alias-attribute",
    "description": "Register morph aliases for your laravel models using php 8 attributes",
    "license": "MIT",
    "keywords": [
        "laravel",
        "morph"
    ],
    "authors": [
        {
            "name": "Dimitris Karapanos",
            "email": "karapanos.dimitris@gmail.com"
        }
    ],
    "homepage": "https://github.com/gpanos/laravel-morph-alias-attribute",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Gpanos\\MorphAliasAttribute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpanos\\MorphAliasAttribute\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gpanos\\MorphAliasAttribute\\MorphAliasAttributeServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
